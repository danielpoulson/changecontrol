{
  "name": "webchangecontrol",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nf start -p 3010",
    "start:prod": "NODE_ENV=production node server/server.js",
    "babel-node": "babel-node --preset=es2015",
    "build:folder": "npm-run-all --parallel build:server copy:*",
    "build:prod": "npm-run-all delete:dist build:folder",
    "build:client": "npm run build --prefix client",
    "delete:dist": "del ./dist",
    "dev:babel": "babel server -d dist/server -watch",
    "dev:nodemon": "nodemon dist/server/server.js",
    "serve:dev": "npm-run-all --parallel dev:*",
    "build:server": "babel server -d dist/server",
    "copy:client": "cpy client/build/*.* dist",
    "copy:clientJS": "cpy client/build/static/js/*.* dist/static/js",
    "copy:clientCSS": "cpy client/build/static/css/*.* dist/static/css",
    "copy:clientmedia": "cpy client/build/static/media/*.* dist/static/media",
    "copy:packjson": "cpy 'package.json' dist",
    "copy:html": "cpy 'server/config/mail.html' dist/server/config/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielpoulson/webchangecontrol.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielpoulson/webchangecontrol/issues"
  },
  "homepage": "https://github.com/danielpoulson/webchangecontrol#readme",
  "dependencies": {
    "body-parser": "^1.16.1",
    "cookie-parser": "^1.4.3",
    "crypto": "^0.0.3",
    "del": "^2.2.2",
    "ejs": "^2.5.6",
    "express": "^4.14.1",
    "express-csv": "^0.6.0",
    "express-session": "^1.15.1",
    "foreman": "^2.0.0",
    "json2csv": "^3.7.3",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "mongoose": "^4.8.4",
    "mongoose-to-csv": "^0.1.1",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nodemailer": "^2.4.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-latest": "^6.22.0"
  }
}
